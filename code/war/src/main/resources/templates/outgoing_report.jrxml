	<subDataset name="Signer">
		<parameter name="docId" class="java.lang.Integer" isForPrompting="false"/>
		<queryString>
			<![CDATA[SELECT
     group_concat(concat(person.`lastName`," ",person.`firstName`," ",person.`middleName`,"")separator " ") AS fio
FROM
     `dms_outgoing_documents` dms_outgoing_documents INNER JOIN `dms_outgoing_documents_signers` dms_outgoing_documents_signers ON dms_outgoing_documents.`id` = dms_outgoing_documents_signers.`id`
     INNER JOIN `dms_system_persons` person ON dms_outgoing_documents_signers.`signer_id` = person.`id`
where dms_outgoing_documents.`id`=$P{docId}]]>
		</queryString>
		<field name="fio" class="java.lang.String"/>
	</subDataset>
	<parameter name="StartDate" class="java.util.Date"/>
	<parameter name="EndDate" class="java.util.Date"/>
	<parameter name="ManagerId" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     contragents.`shortName` AS contragents_shortName,
     dms_outgoing_documents.`registrationDate` AS dms_outgoing_documents_registrationDate,
     documents_forms.`value` AS documents_forms_value,
     dms_outgoing_documents.`id` AS dms_outgoing_documents_id,
     dms_outgoing_documents.`deleted` AS dms_outgoing_documents_deleted,
     dms_outgoing_documents.`registrationNumber` AS dms_outgoing_documents_registrationNumber,
     dms_outgoing_documents.`shortDescription` AS dms_outgoing_documents_shortDescription,
     dms_outgoing_documents.`status_id` AS dms_outgoing_documents_status_id,
     dms_outgoing_documents_contragents.`dms_outgoing_documents_id` AS dms_outgoing_documents_contragents_dms_outgoing_documents_id,
     dms_outgoing_documents_contragents.`recipientContragents_id` AS dms_outgoing_documents_contragents_recipientContragents_id,
     dms_outgoing_documents_contragents.`ID` AS dms_outgoing_documents_contragents_ID
FROM
     `dms_outgoing_documents` dms_outgoing_documents INNER JOIN `documents_forms` documents_forms ON dms_outgoing_documents.`form_id` = documents_forms.`id`
     INNER JOIN `dms_outgoing_documents_contragents` dms_outgoing_documents_contragents ON dms_outgoing_documents.`id` = dms_outgoing_documents_contragents.`dms_outgoing_documents_id`
     INNER JOIN `contragents` contragents ON dms_outgoing_documents_contragents.`recipientContragents_id` = contragents.`id`
     inner join dms_outgoing_documents_signers signers on signers.`id`=dms_outgoing_documents.`id`
WHERE
     dms_outgoing_documents.`deleted` = 0
 AND DATE(dms_outgoing_documents.`registrationDate`) >= DATE($P{StartDate})
 AND DATE(dms_outgoing_documents.`registrationDate`) <= DATE($P{EndDate})
     and signers.`signer_id` = $P{ManagerId}
ORDER BY
     dms_outgoing_documents.`registrationDate` ASC]]>
	</queryString>
