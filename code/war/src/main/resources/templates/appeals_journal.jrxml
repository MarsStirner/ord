	<queryString>
		<![CDATA[SELECT
     dms_request_documents.`id` AS dms_request_documents_id,
     dms_request_documents.`appendixiesCount` AS dms_request_documents_appendixiesCount,
     dms_request_documents.`copiesCount` AS dms_request_documents_copiesCount,
     dms_request_documents.`creationDate` AS dms_request_documents_creationDate,
     dms_request_documents.`deleted` AS dms_request_documents_deleted,
     dms_request_documents.`deliveryDate` AS dms_request_documents_deliveryDate,
     dms_request_documents.`executionDate` AS dms_request_documents_executionDate,
     dms_request_documents.`receivedDocumentDate` AS dms_request_documents_receivedDocumentDate,
     dms_request_documents.`receivedDocumentNumber` AS dms_request_documents_receivedDocumentNumber,
     dms_request_documents.`registrationDate` AS dms_request_documents_registrationDate,
     dms_request_documents.`registrationNumber` AS dms_request_documents_registrationNumber,
     dms_request_documents.`senderFirstName` AS dms_request_documents_senderFirstName,
     dms_request_documents.`senderLastName` AS dms_request_documents_senderLastName,
     dms_request_documents.`senderMiddleName` AS dms_request_documents_senderMiddleName,
     dms_request_documents.`sheetsCount` AS dms_request_documents_sheetsCount,
     dms_request_documents.`shortDescription` AS dms_request_documents_shortDescription,
     dms_request_documents.`status_id` AS dms_request_documents_status_id,
     dms_request_documents.`deliveryType_id` AS dms_request_documents_deliveryType_id,
     dms_request_documents.`form_id` AS dms_request_documents_form_id,
     dms_request_documents.`nomenclature_id` AS dms_request_documents_nomenclature_id,
     dms_request_documents.`region_id` AS dms_request_documents_region_id,
     kladr_regions.`value` AS kladr_regions_value,
     kladr_regions.`category` AS kladr_regions_category,
     kladr_regions.`description` AS kladr_regions_description,
     dms_delivery_types.`id` AS dms_delivery_types_id,
     dms_delivery_types.`deleted` AS dms_delivery_types_deleted,
     dms_delivery_types.`value` AS dms_delivery_types_value,
     dms_system_persons.`lastName` AS dms_system_persons_lastName,
     dms_system_persons.`middleName` AS dms_system_persons_middleName,
     dms_system_persons.`firstName` AS dms_system_persons_firstName,
     documents_forms.`value` AS documents_forms_value,
     documents_forms.`category` AS documents_forms_category
FROM
     `dms_request_documents` dms_request_documents INNER JOIN `kladr_regions` kladr_regions ON dms_request_documents.`region_id` = kladr_regions.`id`
     INNER JOIN `dms_delivery_types` dms_delivery_types ON dms_request_documents.`deliveryType_id` = dms_delivery_types.`id`
     INNER JOIN `dms_request_documents_executors` dms_request_documents_executors ON dms_request_documents.`id` = dms_request_documents_executors.`id`
     INNER JOIN `documents_forms` documents_forms ON dms_request_documents.`form_id` = documents_forms.`id`
     INNER JOIN `dms_system_persons` dms_system_persons ON dms_request_documents_executors.`executor_id` = dms_system_persons.`id`
WHERE
     date(dms_request_documents.`registrationDate`) >= date($P{StartDate})
     and date(dms_request_documents.`registrationDate`) <= date($P{EndDate})
     and documents_forms.`value` like 'Жалоба'
     and documents_forms.`category` like 'Обращения граждан']]>
	</queryString>
