	<subDataset name="Table Dataset 1">
		<parameter name="taskId" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT
     wf_history.`startDate` AS wf_history_startDate,
     wf_history.`to_status_id` AS wf_history_to_status_id
FROM
     `dms_tasks` dms_tasks
     LEFT OUTER JOIN `dms_tasks_executors` dms_tasks_executors ON dms_tasks.`id` = dms_tasks_executors.`id`
     inner JOIN `dms_task_history` dms_task_history ON dms_tasks.`id` = dms_task_history.`task_id`
     inner JOIN `wf_history` wf_history ON dms_task_history.`history_entry_id` = wf_history.`id` and
     wf_history.`to_status_id` = 3
where dms_tasks.`id`=$P{taskId}]]>
		</queryString>
		<field name="wf_history_startDate" class="java.sql.Timestamp"/>
		<field name="wf_history_to_status_id" class="java.lang.Integer"/>
	</subDataset>
	<parameter name="StartDate" class="java.util.Date"/>
	<parameter name="EndDate" class="java.util.Date"/>
	<parameter name="AuthorId" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
     dms_system_persons.`lastName` AS dms_system_persons_lastName,
     (CASE WHEN wf.to_status_id=3 THEN wf.startDate ELSE null END) AS done_date,
     dms_tasks.`deleted` AS dms_tasks_deleted,
     dms_tasks.`executionDate` AS dms_tasks_executionDate,
     dms_tasks.`taskNumber` as dms_tasks_number,
     dms_tasks.`creationDate` AS dms_tasks_creationDate,
     dms_tasks.`shortDescription` AS dms_tasks_shortDescription,
     dms_system_persons.`middleName` AS dms_system_persons_middleName,
     dms_system_persons.`firstName` AS dms_system_persons_firstName,
     dms_tasks_executors.`executor_id` AS dms_tasks_executors_executor_id,
     dms_tasks_executors.`id` AS dms_tasks_executors_id,
     dms_system_persons_A.`firstName` AS dms_system_persons_A_firstName_executor,
     dms_system_persons_A.`lastName` AS dms_system_persons_A_lastName_executor,
     dms_system_persons_A.`middleName` AS dms_system_persons_A_middleName_executor,
     dms_tasks.`controlDate` AS dms_tasks_controlDate,
     dms_tasks.`status_id` AS dms_tasks_status_id,
dms_tasks.`id` as dms_tasks_id
FROM
     `dms_tasks` dms_tasks
LEFT OUTER join dms_task_history hist on hist.task_id=dms_tasks.id
LEFT OUTER join wf_history wf on wf.id=hist.history_entry_id and wf.to_status_id=3
     INNER JOIN `dms_system_persons` dms_system_persons ON dms_tasks.`author_id` = dms_system_persons.`id`
     LEFT OUTER JOIN `dms_tasks_executors` dms_tasks_executors ON dms_tasks.`id` = dms_tasks_executors.`id`
     RIGHT OUTER JOIN `dms_system_persons` dms_system_persons_A ON dms_tasks_executors.`executor_id` = dms_system_persons_A.`id`
WHERE
     DATE(dms_tasks.`creationDate`) >= DATE($P{StartDate})
 AND DATE(dms_tasks.`creationDate`) <= DATE($P{EndDate})
 AND dms_tasks.`deleted` = 0
and dms_tasks.`author_id` = $P{AuthorId}
group by dms_tasks.id
ORDER BY
     dms_tasks.`controlDate` ASC]]>
	</queryString>
