	<subDataset name="ExecDate">
		<parameter name="DocId" class="java.lang.Integer">
			<defaultValueExpression><![CDATA[]]></defaultValueExpression>
		</parameter>
		<queryString>
			<![CDATA[SELECT dms_outgoing_documents.registrationNumber
     , dms_outgoing_documents.registrationDate
FROM
  dms_incoming_documents
LEFT JOIN dms_outgoing_documents
ON dms_incoming_documents.id = substring(dms_outgoing_documents.reason_document_id, 10,10)
WHERE
  dms_incoming_documents.`id` = $P{DocId}]]>
		</queryString>
		<field name="registrationNumber" class="java.lang.String"/>
		<field name="registrationDate" class="java.sql.Timestamp"/>
	</subDataset>
	<parameter name="StartDate" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="EndDate" class="java.util.Date">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<parameter name="ManagerId" class="java.lang.String">
		<defaultValueExpression><![CDATA[]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT
 dms_incoming_documents.`registrationDate` as registrationDate,
dms_incoming_documents.`id` as dms_incoming_documents_id,
     dms_incoming_documents.`executionDate` AS dms_incoming_documents_executionDate,
dms_incoming_documents.`id` as docId,
     dms_incoming_documents.`receivedDocumentNumber` AS dms_incoming_documents_receivedDocumentNumber,
     dms_incoming_documents.`receivedDocumentDate` AS dms_incoming_documents_receivedDocumentDate,
     contragents.`shortName` AS contragents_shortName,
     dms_incoming_documents.`registrationNumber` AS dms_incoming_documents_registrationNumber,
     dms_incoming_documents.`shortDescription` AS dms_incoming_documents_shortDescription,
     dms_incoming_documents.`creationDate` AS dms_incoming_documents_creationDate,
	 CONCAT_WS(' ', controller.lastName, controller.firstName, controller.middleName ) AS controllerFio,
      group_concat(distinct concat(person.`lastName`," ",LEFT(person.`firstName`,1),"."," ",LEFT(person.`middleName`,1),".","") separator ", ") AS fio,
     dms_incoming_documents_executors.`dms_incoming_documents_id` AS dms_incoming_documents_executors_dms_incoming_documents_id,
     dms_incoming_documents_executors.`executors_id` AS dms_incoming_documents_executors_executors_id,
     dms_incoming_documents_executors.`ID3` AS dms_incoming_documents_executors_ID3
FROM
     `dms_incoming_documents` dms_incoming_documents INNER JOIN `contragents` contragents ON dms_incoming_documents.`contragent_id` = contragents.`id`
     INNER JOIN `dms_incoming_documents_executors` dms_incoming_documents_executors ON dms_incoming_documents.`id` = dms_incoming_documents_executors.`dms_incoming_documents_id`
     INNER JOIN `dms_system_persons` person ON dms_incoming_documents_executors.`executors_id` = person.`id`
     INNER JOIN `dms_incoming_documents_executors` exec ON person.`id` = exec.`executors_id`
     inner join `dms_system_persons` controller ON dms_incoming_documents.`controller_id` = controller.`id`
WHERE
     date(dms_incoming_documents.`registrationDate`) >= date($P{StartDate})
     and date(dms_incoming_documents.`registrationDate`) <= date($P{EndDate})
     and dms_incoming_documents.executionDate IS NOT NULL
     and dms_incoming_documents.`controller_id` = $P{ManagerId}
     AND dms_incoming_documents.deleted = 0
GROUP BY
     dms_incoming_documents.`id`]]>
	</queryString>
