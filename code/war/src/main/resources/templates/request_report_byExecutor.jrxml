	<subDataset name="Manager" uuid="22effc06-93c2-458a-a1f7-3fd40e31b37c">
		<parameter name="executorId" class="java.lang.String" isForPrompting="false"/>
		<queryString>
			<![CDATA[SELECT
                    CONCAT_WS(' ' , user.lastName, user.firstName, user.middleName) AS 'managerInfo'
                    FROM
                    dms_system_persons user
                    WHERE user.id = $P{executorId}]]>
		</queryString>
		<field name="managerInfo" class="java.lang.String"/>
	</subDataset>
	<parameter name="begDate" class="java.util.Date"/>
	<parameter name="endDate" class="java.util.Date"/>
	<parameter name="executorId" class="java.lang.Integer"/>
	<parameter name="organisation.shortName" class="java.lang.String"/>
	<parameter name="organisation.fullName" class="java.lang.String"/>
	<queryString>
		<![CDATA[SELECT
                requestDocumentId,
                requestDocumentRegistrationNumber,
                kladrId,
                kladrValue,
                kladrCategory,
                senderInfo,
                requestDocumentShortDescription,
                outgoingDocumentId,
                outgoingDocumentRegistrationNumber,
                outgoingDocumentShortDescription,
                executorId,
                checkDate
            FROM
                report_requests_by_executor
            WHERE
                executorId = $P{executorId}
                AND
                DATE(checkDate) >= DATE($P{begDate})
                AND
                DATE(checkDate) <= DATE($P{endDate})]]>
	</queryString>
	<field name="requestDocumentId" class="java.lang.Integer"/>
